{ lib }: let

# ---------------------------------------------------------------------------- #

  checkUrl = lib.ytypes.Uri.Strings.uri_ref.check;

# ---------------------------------------------------------------------------- #

  tests = {

# ---------------------------------------------------------------------------- #

    testStringTypeChecker = {
      expr     = checkUrl "https://google.com";
      expected = "https://google.com";
    };


    testUriParseUnparse = {
      expr     = toString ( lib.liburi.parseFullUrl "https://google.com" );
      expected = "https://google.com";
    };


# ---------------------------------------------------------------------------- #

  };  # End Tests


# ---------------------------------------------------------------------------- #

in tests


